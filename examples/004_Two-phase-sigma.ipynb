    {
      "cell_type": "code",
      "execution_count": 10,
      "source": [
        "from josie.boundary import Line\n",
        "from josie.math import Direction\n",
        "\n",
        "left = Line([0, 0], [0, 1])\n",
        "bottom = Line([0, 0], [1, 0])\n",
        "right = Line([1, 0], [1, 1])\n",
        "top = Line([0, 1], [1, 1])\n",
        "\n",
        "# Left-right is periodic\n",
        "left, right = make_periodic(left, right, Direction.X)\n",
        "\n",
        "# Top-bottom\n",
        "top.bc = zero_normal_velocity\n",
        "bottom.bc = zero_normal_velocity"
      ],
      "outputs": [],
