        "from josie.io.write.writer import XDMFWriter\n",
        "from josie.io.write.strategy import TimeStrategy\n",
        "\n",
        "# Instantiate the solver\n",
        "solver = SigmaSolver(mesh, FinalScheme(eos))\n",
        "solver.init(init_fun)\n",
        "\n",
        "solver.plot()\n",
        "solver.show(\"rho\")"
