      "source": [
        "from josie.mesh import Mesh\n",
        "from josie.mesh.cell import SimpleCell\n",
        "\n",
        "mesh = Mesh(left, bottom, right, top, SimpleCell)\n",
        "mesh.interpolate(150, 150)\n",
        "mesh.generate()"
